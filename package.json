{
  "name": "praizanee-clickbait",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "wireit",
    "build": "wireit",
    "preview": "wireit",
    "check": "wireit",
    "check:watch": "wireit",
    "lint": "wireit",
    "format": "wireit"
  },
  "devDependencies": {
    "@fontsource/noto-sans": "^5.0.3",
    "@fontsource/noto-sans-thai": "^5.0.3",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-cloudflare": "^2.3.0",
    "@sveltejs/kit": "^1.20.4",
    "autoprefixer": "^10.4.14",
    "pnpm": "^8.6.5",
    "postcss": "^8.4.24",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^4.0.0",
    "svelte-check": "^3.4.3",
    "svelte-meta-tags": "^2.8.0",
    "tailwindcss": "^3.3.2",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.3.6",
    "wireit": "^0.9.5"
  },
  "type": "module",
  "wireit": {
    "dev": {
      "command": "vite dev"
    },
    "build": {
      "command": "vite build"
    },
    "preview": {
      "command": "vite preview"
    },
    "check": {
      "command": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json"
    },
    "check:watch": {
      "command": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
    },
    "lint": {
      "command": "prettier --plugin-search-dir . --check ."
    },
    "format": {
      "command": "prettier --plugin-search-dir . --write ."
    }
  },
  "packageManager": "pnpm@8.6.5"
}